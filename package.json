{
  "name": "easydebug",
  "version": "1.0.0",
  "description": "Javascript debug tool for daily development",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "electron src/main/main.js",
    "dev:renderer": "webpack serve --config build/webpack.renderer.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config build/webpack.main.config.js",
    "build:renderer": "webpack --config build/webpack.renderer.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofix/EasyDebug.git"
  },
  "keywords": [
    "debug",
    "tool"
  ],
  "author": "ofix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ofix/EasyDebug/issues"
  },
  "homepage": "https://github.com/ofix/EasyDebug#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@vue/compiler-sfc": "^3.5.24",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "electron": "^39.2.1",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.2.0",
    "sass": "^1.94.1",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "element-plus": "^2.11.8",
    "pinia": "^3.0.4",
    "vue": "3.5.24",
    "vue-router": "^4.6.3"
  }
}
